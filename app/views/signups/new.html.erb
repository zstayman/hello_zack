<%= form_for @signup do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :company %>
<%= f.text_field :company %>
<%= f.label :phone_number %>
<%= f.telephone_field :phone_number %>
<%= f.submit "Hello" %>
<% end %>
